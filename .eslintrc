{
  "env": {
      "node": true, 
      "browser": true, 
      "es6": true 
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types":[0],
    "react/require-default-props": [0],
    "react/prefer-stateless-function": [0],
    "react/destructuring-assignment": [0],
    "arrow-body-style": [0]
  },
  "parser": "babel-eslint", 
  "extends": "airbnb"
}